  "bugs": {
    "url": "https://github.com/guardian/janus-app/issues"
  },
  "homepage": "https://github.com/guardian/janus-app#readme",
  "dependencies": {
    "@fontsource/roboto": "^5.1.1",
    "dompurify": "^3.2.4",
    "material-icons": "^1.13.13",
    "materialize-css": "^1.0.0",
    "parcel": "^2.13.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@guardian/eslint-config": "^11.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^30.0.2",
    "eslint": "^9.25.1",
    "eslint-plugin-jest": "^29.0.1",
